pipeline {
    agent {
        docker {
            image 'postman/newman:alpine'  // Utilisation de l'image officielle Newman
           
        }
    }
    stages {
        stage('Vérifier Newman') {
            steps {
                echo 'Vérification de la version de Newman...'
                sh 'newman -v'  
            }
        }
        
     
    }
   
}
